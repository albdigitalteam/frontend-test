{
  "extends": "@react-native-community",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "off",
    "semi": 0,
    "trailing-comma": [ true, {
      "singleline": "never",
      "multiline": {
        "objects": "ignore",
        "arrays": "always",
        "functions": "never",
        "typeLiterals": "ignore"
      }
    }]
  }
}
